<?xml version="1.0" encoding="UTF-8"?>
<ApiarySpecimenRecordSet>
  <ApiarySpecimenRecord>
    <basisOfRecord>preservedSpecimen</basisOfRecord>
    <CollectionEvent>
      <!--The following terms are from DwC Occurrence/Apiary Class. -->
      <occurrenceRemarks/>
      <recordNumber/>
      <recordedBy/>
      <sex/>
      <ifeStage/>
      <individualID/>
      <individualCount/>
      <reproductiveCondition/>
      <behavior/>
      <primaryLabelHeader/>
      <primaryLabelFooter/>
      <establishmentMeans/>
      <occurrenceStatus/>
      <previousIdentifications/>
      <associatedTaxa/>
      <primaryRecord/>

      <!--The following terms are from DwC Event Class. -->
      <eventDate/>
      <verbatimEventDate/>
      <habitat/>
      <fieldNotes/>
      <eventRemarks/>

      <!--The following terms are from DwC/Apiary Location Class. -->
      <higherGeography/>
      <continent/>
      <country/>
      <countryCode/>
      <stateProvince/>
      <county/>
      <municipality/>
      <locality/>
      <verbatimElevation/>
      <minimumElevationInMeters/>
      <maximumElevationInMeters/>
      <verbatimCoordinates/>
      <verbatimLatitude/>
      <verbatimLongitude/>
      <verbatimCoordinateSystem/>
      <verbatimSRS/>
      <decimalLatitude/>
      <decimalLongitude/>
      <geodeticDatum/>
      <coordinateUncertaintyInMeters/>
      <waterbody/>
      <islandGroup/>
      <island/>
      <verbatimLocality/>
      <locationRemarks/>
      <coordinatePrecision/>
      <pointRadiusSpatialFit/>
      <georeferenceSources/>
      <georeferenceVerificationStatus/>
      <georeferenceRemarks/>
      <georeferencedBy/>

      <!--The following terms are from DwC MeasurementOrFact Class. -->
      <measurementType/>
      <measurementValue/>
      <measurementAccuracy/>
      <measurementUnit/>
      <measurementDeterminedDate/>
      <measurementDeterminedBy/>
      <measurementMethod/>
      <measurementRemarks/>
    </CollectionEvent>
    <AnnotationEvents>
      <AnnotationEvent>
        <!-- The following terms are from DwC RecordLevel Terms -->
        <institutionCode/>
        <collectionCode/>
        <datasetName/>
        <ownerInstitutionCode/>
        <dynamicProperties/>
        <previousOwners/>

        <!--The following terms are from DwC Occurrence Class. -->
        <catalogNumber/>
        <preparations/>
        <disposition/>
        <otherCatalogNumbers/>
        <associatedMedia/>
        <associatedReferences/>
        <associatedOccurrences/>
        <associatedSequences/>

        <!--The following terms are from DwC/Apiary Identification Class. -->
        <identifiedBy/>
        <identificationQualifier/>
        <typeStatus/>
        <dateIdentified/>
        <identificationReferences/>
        <identificationRemarks/>
        <verbatimDateIdentified/>
        <primaryIdentifier/>
        <identificationStatus/>
        <typeDesignation/>

        <!-- The following terms are from DwC/Apiary Taxon Class. -->
        <scientificName/>
        <higherClassification/>
        <kingdom/>
        <phylum/>
        <class/>
        <order/>
        <family/>
        <genus/>
        <specificEpithet/>
        <infraspecificEpithet/>
        <scientificNameAuthorship/>
        <vernacularName/>
        <nomenclaturalCode/>
        <originalNameUsage/>
        <namePublishedIn/>
        <subgenus/>
        <taxonRank/>
        <verbatimTaxonRank/>
        <taxonomicStatus/>
        <nomenclaturalStatus/>
        <taxonRemarks/>
        <vernacularNameLanguage/>
        <verbatimScientificName/>
        <verbatimHigherClassification/>

        <!-- The following terms are from Apiary Annotation Class. This is an Apiary unique class.-->
        <basisOfAnnotation/>
        <annotatedBy/>
        <primaryAnnotator/>
        <annotationRemarks/>
        <verbatimDateAnnotated/>
        <dateAnnotated/>
        <annotationReferences/>
      </AnnotationEvent>
    </AnnotationEvents>
  </ApiarySpecimenRecord>
</ApiarySpecimenRecordSet>
