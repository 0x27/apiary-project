/* general
--------------------------- */

body {
	margin: 0 auto;
	text-align: center;
	background: #fff url(../img/bg_body.gif) repeat-x top left;
	color: #333;
	font-family: Arial, sans-serif;
	font-size: 100%;
}

#page {
	margin: 0 auto;
	text-align: center;
	width: 940px;
}

#skip {
	display: block;
	text-indent: -9999px;
	position: absolute;
	overflow: hidden; /* bug fix for IE 7 */
}

a {
   outline: 0;
}

