.widget-header {
	text-align:left;
	background-color: #ccc;
	size:auto;
	border-top:1px solid #dadada;
	border-bottom:1px solid #c0c0c0;
	height: 40px;
	padding: 5px;
}

.widget-title  {
	text-align:left;
	background-color: #bbb;
	size:auto;
	border-top:1px solid #cacaca;
	border-bottom:1px solid #b0b0b0;
	height: 20px;
}

.widget-footer  {
	text-align:left;
	background-color: #bbb;
	size:auto;
	border-top:1px solid #cacaca;
	border-bottom:1px solid #b0b0b0;
	height: 20px;
}

.widget-header  h3{
	font-size: 14px;
	width: 200px;
	float: left;
	height: 38px;
}

.widget-control {
	float:left;
	height: 38px;
	width: 38px;
}


.widget-subheader {
	background-color: #eee;
	size:auto;
	border-top:1px solid #f4f4f4;
	border-bottom:1px solid #e0e0e0;
}

.widget-subheader  h3{
	font-size:12px;
}

.widget-content {
	display:none;
}

.widget-subcontent {
	font-size:12px;
}

.roi-section  {
	text-align:left;
	background-color: #ccc;
	size:auto;
	border-top:1px solid #dadada;
	border-bottom:1px solid #c0c0c0;
	height: 80px;
	padding: 5px;
}

.roi-preview {
	float: left;
	height:60px;
	width:80px;
	margin:10px;
	background-color:#ddd;
	border-top:1px solid #eaeaea;
	border-left:1px solid #e0e0e0;
	border-right:1px solid #d8d8d8;
	border-bottom:1px solid #d0d0d0;
}

.roi-controls {
	float: left;
	height:60px;
	width:140px;
	margin:10px;
	background-color:#ddd;
	border-top:1px solid #eaeaea;
	border-left:1px solid #e0e0e0;
	border-right:1px solid #d8d8d8;
	border-bottom:1px solid #d0d0d0;
	font-size: 10px;
}

.roi-preview {
	padding:4px;
}

#queue-info {
	height: auto;
	width: 150px;
