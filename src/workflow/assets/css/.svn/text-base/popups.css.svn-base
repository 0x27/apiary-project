.clearleft {
	clear: left;
}

.clearright {
	clear: right;
}

.clear {
	clear: both;
}

.clearfix:after {
	content: "."; 
	display: block; 
	height: 0; 
	clear: both; 
	visibility: hidden;
}

.clearfix {display: inline-block;}

/* Hides from IE-mac \*/
* html .clearfix {height: 1%;}
.clearfix {display: block;}
/* End hide from IE-mac */

hr, .print-only {
	display: none;
}

.wai {
	display: block;
	text-indent: -9999px;
	position: absolute;
	overflow: hidden; /* bug fix for IE 7 */
	width: 0;
	height: 0;
}


