.pagination {
	/* text-align: center; 
	margin: .5em 0 1.5em 0;*/
}

.pagination ul {
	margin: 0;
	padding: 0 !important;
	/* text-align: center; */
	list-style: none;
}

.pagination li {
	display: inline; 
	padding: 0 !important;
	background-image: none !important;
/*	background: url(../img/dialog_title.gif) 0 0 no-repeat;*/
	font-size: 12px;
	height:20px;
}

.pagination a.first {
	background: url(../img/icon_first.gif) no-repeat left 50%;
	padding-left: 20px;
}

.pagination a.first.inactive,
.pagination span.first {
	background: url(../img/icon_first_inactive.gif) no-repeat left 50%;
	padding-left: 20px;
	color: #A0A0A0;
}

.pagination a.last {
	background: url(../img/icon_last.gif) no-repeat right 50%;
	padding-right: 20px;
}

.pagination a.last.inactive,
.pagination span.last {
	background: url(../img/icon_last_inactive.gif) no-repeat right 50%;
	padding-right: 20px;
	color: #A0A0A0;
}

.pagination a.next {
	background: url(../img/icon_next.gif) no-repeat right 50%;
	padding-right: 16px;
	height: 20px;
}

.pagination a.next.inactive,
.pagination span.next {
	background: url(../img/icon_next_inactive.gif) no-repeat right 50%;
	padding-right: 16px;
	padding-left: 5px;
	color: #A0A0A0;
    text-shadow: #777 0px 1px 1px;
}

.pagination a.previous {
	background: url(../img/icon_previous.gif) no-repeat left 50%;
	padding-left: 16px;
}

.pagination a.previous.inactive,
.pagination span.previous {
	background: url(../img/icon_previous_inactive.gif) no-repeat left 50%;
	padding-left: 16px;
	padding-right: 5px;
	color: #A0A0A0;
}

.pagination a {
	padding: 1px 4px 2px 4px;
	height: 20px;
	background: url(../img/dialog_title.gif) 0 0 repeat-x;
	border: 1px solid #cdcdcd;
	color: #000;
	text-decoration: none;
    text-shadow: #fff 1px 1px 1px;

}

.pagination a:hover {
	border: 1px solid #000;
	color: #000;
	background-color: #e9e9e9;
}

.pagination a.current {
	background-color: #777;
	color: #222 !important;
	padding: 2px 5px 3px 5px;
	border: 1px solid #777;
    text-shadow: #444 1px 1px 1px;
}

.pagination a.action {
    font-size:12px;
    float:right;
    margin-top:-20px; 
    height: 16px;
}

.pagination a.action#cancel {
    margin-right:2px;   
}